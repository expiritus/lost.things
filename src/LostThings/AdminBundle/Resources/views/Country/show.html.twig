{% extends 'LostThingsAdminBundle::layout.html.twig' %}

{% block body -%}
    <h1>Country</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Страна</th>
                <td>{{ entity.country }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_country_') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_country__edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
